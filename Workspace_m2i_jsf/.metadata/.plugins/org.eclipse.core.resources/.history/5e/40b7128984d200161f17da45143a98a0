@ManagedBean(name = "playground", eager = true)
@SessionScoped
public class ControleurPlayground {
	
	public List<Case> positions = new ArrayList<Case>();
	public ControleurPlayground() {
		
		for (i = 0, i < 10, i++) {
			
			lescases.add(i);
		}
	}

}
